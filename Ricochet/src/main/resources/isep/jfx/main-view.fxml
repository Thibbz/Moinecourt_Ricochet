<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox prefHeight="661.0" prefWidth="936.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="isep.jfx.MainController">
   <children>
      <HBox>
         <children>
            <VBox prefHeight="160.0" prefWidth="580.0">
               <children>
                  <Button mnemonicParsing="false" onAction="#showPlayerView" prefWidth="70.0" text="Jouer">
                     <VBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </VBox.margin>
                  </Button>
                  <Label fx:id="nbCoupLabel" prefHeight="18.0" prefWidth="150.0" text="Label" />
                  <Label fx:id="timer" text="Timer : 0.0s" />
               </children>
            </VBox>
            <AnchorPane fx:id="gamePane" minHeight="0.0" minWidth="0.0" prefHeight="160.0" prefWidth="100.0" stylesheets="@style.css">
               <children>
                  <Pane fx:id="corner1" minHeight="330.0" minWidth="330.0" prefHeight="200.0" prefWidth="200.0" rotate="90.0" styleClass="corner4" />
                  <Pane fx:id="corner3" layoutY="330.0" minHeight="360.0" minWidth="330.0" prefHeight="200.0" prefWidth="200.0" styleClass="corner1" stylesheets="@style.css" />
                  <Pane fx:id="corner4" layoutX="330.0" layoutY="330.0" minHeight="330.0" minWidth="330.0" prefHeight="200.0" prefWidth="200.0" rotate="270.0" styleClass="corner2" stylesheets="@style.css" />
                  <Pane fx:id="corner2" layoutX="330.0" minHeight="330.0" minWidth="330.0" prefHeight="200.0" prefWidth="200.0" rotate="180.0" styleClass="corner3" />
                  <GridPane fx:id="boardPane" layoutX="6.0" layoutY="8.0" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="720.0" minWidth="760.0" prefHeight="720.0" prefWidth="760.0">
                  </GridPane>
               </children></AnchorPane>
            <Label fx:id="statusLabel" alignment="TOP_RIGHT" contentDisplay="RIGHT" maxWidth="1.7976931348623157E308" prefHeight="18.0" prefWidth="1220.0" text="Label" textFill="RED" />
            <!--<GridPane fx:id="boardPane" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="640.0" prefWidth="640.0">
      </GridPane>-->

         </children>
      </HBox>
   </children>
</VBox>
